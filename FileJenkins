pipeline {
agent any
stages{
stage('clone and repo'){
steps {
cleanWs()
bat "git clone https://github.com/MedHammedi/CI-Project/"
}}
stage('Build and Test'){
steps{ 
bat "mvn install -f CI-Project"
}}
stage(' Deploy '){
steps {
bat "mvn package -f CI-Project"
bat "mvn deploy -f CI-Project"
bat "mvn sonar:sonar -f CI-Project"
} }}}
